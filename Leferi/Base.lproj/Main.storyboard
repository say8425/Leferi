                                                            <font key="NSFont" size="12" name="KoPubDotumPM"/>
                                                            <paragraphStyle key="NSParagraphStyle" alignment="left" lineBreakMode="wordWrapping" baseWritingDirection="natural" lineHeightMultiple="1.2"/>
                                                        </attributes>
